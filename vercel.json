{
  "regions": ["lhr1"],
  "functions": {
    "app/api/**/route.ts": {
      "maxDuration": 60
    },
    "app/**/page.tsx": {
      "maxDuration": 60
    }
  },
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://donatheresa.com/$1", 
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "www.donatheresa.com"
        }
      ]
    }
  ]
}